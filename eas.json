{
  "cli": {
    "version": ">= 2.4.0"
  },
  "build": {
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "yarn":"1.22.19",
      "node": "16.10.0",
      "env":  {
        "API_URL": "gigsstory-api-prod.eba-c662d9mj.us-east-2.elasticbeanstalk.com",
        "ENV": "prod"
      }
    },
    "staging": {
      "android": {
        "buildType": "apk"
      },
      "extends": "production",
      "distribution": "internal",
      "env": {
        "API_URL": "http://gigsstory-api-dev.us-east-2.elasticbeanstalk.com",
        "ENV": "stg"
      }
    },
    "development": {
      "extends": "production",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_URL": "http://gigsstory-api-dev.us-east-2.elasticbeanstalk.com",
        "ENV": "dev"
      }
    }
  },
  "submit": {
    "production": {}
  }
